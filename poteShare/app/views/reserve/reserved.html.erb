 
  <table>
        <tr>
            <th>ルーム画像</th>
            <th>ルーム名</th>
            <th>ルーム紹介</th>
            <th>合計料金	</th>
            <th>開始日	</th>
            <th>終了日	</th>
            <th>予約確定日時</th>
            
        </tr>
          <tr>
           <% @reserve.each do |share| %>
            <td>
             <% if share.image.url%>
              <%= link_to image_tag(share.image.url ,width: "240px",height: "140px" ),reserve_room_path(share)%>
             <%else%>
             <p>画像がありません</p>
             <%end%>
            </td>
            <td><%= share.title %></td>
            <td><%= share.comment %></td>
            <td><%= share.price%></td>
            <td><%= share.start %></td>
            <td><%= share.end%></td>
            <td><%= share.updated_at.strftime("%Y年%m月%d日%-H時%M分")%></td>
          
          </tr>
           <% end %>
  </table>
           
           
  