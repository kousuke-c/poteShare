 <table class = 'room-left'>
    
        
        <td><%= @share.price %></td>
      </tr>
      <tr>
        
        <td><%= @reserve.start.to_s(:datetime_jp) %></td>
      </tr>
       <tr>
        
        <td><%= @reserve.end.to_s(:datetime_jp) %></td>
      </tr>
      <tr>
       
        <td><%= @day %></td>
      </tr>
      <tr>
       
        <td><%= @reserve.people %></td>
        
        <tr>
       
        <td><%= @price %></td>
      </tr>
      </tr>
    </table>
   <% @image= @images %>

<%= form_for @reserve, method: :post, url: reserve_comprete_path(@reserve) do |f| %>
	<%= f.hidden_field :price,value: @price%>
    	<%= f.hidden_field :title ,value: @share.title%>
    	<%= f.hidden_field :area ,value: @share.area%>   	
    	<%= f.hidden_field :comment,value: @share.comment %>
    	<%= f.hidden_field :image,value: images %>
     <%= f.hidden_field :start ,value: @reserve.start%>   	
    	<%= f.hidden_field :end,value: @reserve.end %>
     <%= f.hidden_field :people,value: @reserve.people %>
	    <%= f.submit '送信する' %>
<% end %>

 <%= form_for @reserve, method: :post, url: reserve_back_path(@share) do |f| %>
	<%= f.hidden_field :price,value: @price%>
    	<%= f.hidden_field :title ,value: @share.title%>
    	<%= f.hidden_field :area ,value: @share.area%>   	
    	<%= f.hidden_field :comment,value: @share.comment %>
    	<%= f.hidden_field :image,value: @share.image.url%>
             <%= f.hidden_field :start ,value: @reserve.start%>   	
    	<%= f.hidden_field :end,value: @reserve.end %>
<%= f.hidden_field :people,value: @reserve.people %>
       
	<%= f.submit '入力画面に戻る' %>
<% end %>